<folder folded="yes">
    <strategy>
<!--Strategy config file-->
<!--The amount of time used for hysteresis.  
The amount of time the ball has to be in the defense zone before declaring such.-->
        <DZONE_HYSTERESIS_DURATION>0.3</DZONE_HYSTERESIS_DURATION>
<!--The amount of time alone with the ball before declaring possesion.-->
        <POSSESSION_US_HYSTERESIS_DURATION>0.2</POSSESSION_US_HYSTERESIS_DURATION>
        <POSSESSION_THEM_HYSTERESIS_DURATION>0.3</POSSESSION_THEM_HYSTERESIS_DURATION>     
<!--Switches that control aspects of the play execution system.-->
        <PLAYEXEC_OPPORTUNISM>1</PLAYEXEC_OPPORTUNISM><!--boolean-->
        <PLAYEXEC_ROLE_SWITCHING>1</PLAYEXEC_ROLE_SWITCHING><!--boolean-->
        <PLAYEXEC_FIXED_ROLES>0</PLAYEXEC_FIXED_ROLES><!--boolean-->
        <PLAYEXEC_DEBUG_ROLE_SWITCHING>0</PLAYEXEC_DEBUG_ROLE_SWITCHING><!--boolean-->
<!--The MIN amount of time a play has to run before it can be considered the responsible play.  The MAX is the maximum amount of time after finishing the play remains the responsible one.-->
        <RESPONSIBLE_MIN_RUNTIME>2.0</RESPONSIBLE_MIN_RUNTIME><!--s-->
        <RESPONSIBLE_MAX_RUNTIME>4.0</RESPONSIBLE_MAX_RUNTIME><!--s-->
<!--The minimum amount of time a play must run before receiving done credit/blame.-->         
        <CREDIT_MIN_RUNTIME>1.0</CREDIT_MIN_RUNTIME><!--s-->
<!--min distance for closest_to_ball_but_not_too_close predicate-->
        <MIN_DIST_TO_BALL>180.0</MIN_DIST_TO_BALL><!--mm-->
<!--Adapt play weights by results.-->
        <PLAYBOOK_ADAPT_WEIGHTS>1</PLAYBOOK_ADAPT_WEIGHTS><!--boolean-->   
<!--Our playbook file.-->
<!-- ************************************************ -->
        <PLAYBOOK>advanced.plb</PLAYBOOK>
    </strategy>
</folder>
<!--Tactic config file-->
<!--TDefend TBlock TMark-->
<folder folded="yes">
    <tactic>
        <DEFENSE_OFF_BALL>90</DEFENSE_OFF_BALL><!--mm-->
        <MARK_OFF_OPPONENT>270</MARK_OFF_OPPONENT><!--mm-->
<!--TGoalie-->
<!--The maximum angle (different from downfield) that we'll face when the ball is in the     corner.
    GOALIE_MAX_ANGLE = 1.3744 # 78.75 deg-->
        <GOALIE_MAX_ANGLE>0.96</GOALIE_MAX_ANGLE><!--55.00 deg-->
<!--The distance off the ball.  This is less than our radius plus the balls
    so the goalie will actually push it out.-->
        <GOALIE_DIST_OFF_BALL>5</GOALIE_DIST_OFF_BALL><!--mm-->
<!--The minimum and maximum distances played from the goal.-->
        <GOALIE_DIST_MIN>100</GOALIE_DIST_MIN><!--mm-->
        <GOALIE_DIST_MAX>225</GOALIE_DIST_MAX><!--mm, DEFENSE_DEPTH + 40-->
<!--parameters for penalty kicks
 match them is for whether to adjust positino to match opponent position
 line is the line to run along before the start
 max: the max y value we will go to to match the opponent-->
        <GOALIE_PENALTY_MATCH_THEM>1</GOALIE_PENALTY_MATCH_THEM><!--true-->
        <GOALIE_PENALTY_LINE>-1333.0</GOALIE_PENALTY_LINE><!--mm-->
        <GOALIE_PENALTY_MAX>167.0</GOALIE_PENALTY_MAX><!--250.0 #mm-->
<!--TShoot-->
<!--The amount we'd prefer to shoot at our previous angle.  If an another
    at least this much bigger appears we'll switch to that.-->
        <SHOOT_AIM_PREF_AMOUNT>0.01745</SHOOT_AIM_PREF_AMOUNT><!--1 degree
0.1221 # 7 deg-->
<!--We make sure the angle tolerance is always this big.-->
<!--SHOOT_MIN_ANGLE_TOLERANCE = 0.1745 # Pi / 16-->
        <SHOOT_MIN_ANGLE_TOLERANCE>0.08725</SHOOT_MIN_ANGLE_TOLERANCE><!--Pi / 32-->
<!--Dribbles to open shot when nothing's open.-->
        <SHOOT_DRIBBLE_IF_NO_SHOT>1</SHOOT_DRIBBLE_IF_NO_SHOT><!--boolean-->
<!--TPass-->
<!--The "width" of our teammate's front for the purpose of aiming a
    pass.  Some connection to it's dribbler's width.-->
        <PASS_TARGET_WIDTH>30.0</PASS_TARGET_WIDTH>
<!--TPositionForDeflection-->
        <POS_DEFLECT_PREF_AMOUNT>0.1221</POS_DEFLECT_PREF_AMOUNT><!--7 deg-->
<!--TPositionForPenalty-->
<!--distance to stand away from ball on penalty kicks-->
        <DISTANCE_FROM_PENALTY_LINE>725.0</DISTANCE_FROM_PENALTY_LINE><!--mm-->
    </tactic>
</folder>
<folder folded="yes">
    <test>
<!--****************************************-->
    </test>
</folder>
<folder folded="yes">
<!--Tracker config file-->
    <tracker>
<!--# BallTracker-->
<!--Sloped Walls-->
        <BALL_WALLS_SLOPED>1</BALL_WALLS_SLOPED><!--true-->
<!--If true ball can go out of bounds.  This is handled by "stopping" the ball
    at the position where it went out-of-bounds.-->
        <BALL_WALLS_OOB>1</BALL_WALLS_OOB><!--true-->
<!--Improbability filtering (if true, see BALL_LIKELIHOOD_THRESHOLD below)-->
        <BALL_IMPROBABILITY_FILTERING>0</BALL_IMPROBABILITY_FILTERING><!--false-->
<!--Output Kalman Errors-->
        <BALL_PRINT_KALMAN_ERROR>0</BALL_PRINT_KALMAN_ERROR><!--false-->
<!--Collision Radii.
    Ball is bounced/stopped when within this radius of teammates and/or 
    opponents.  Setting to zero turns off collision prediction.-->
        <BALL_TEAMMATE_COLLISION_RADIUS>70</BALL_TEAMMATE_COLLISION_RADIUS><!--mm-->
        <BALL_OPPONENT_COLLISION_RADIUS>70</BALL_OPPONENT_COLLISION_RADIUS><!--mm-->
<!--Observation noise in position.-->
        <BALL_POSITION_VARIANCE>36.0</BALL_POSITION_VARIANCE><!--Stddev = 6mm-->
<!--Velocity noise.
    This was deliberately not based on stepsize since it's goal is to 
    account for the ball being hit by robots.-->
        <BALL_VELOCITY_VARIANCE_NO_ROBOT>36.0</BALL_VELOCITY_VARIANCE_NO_ROBOT><!--Stddev = 6mm-->
        <BALL_VELOCITY_VARIANCE_NEAR_ROBOT>250000</BALL_VELOCITY_VARIANCE_NEAR_ROBOT><!--Stddev = 500mm/s-->
<!--This is (Friction_Coef / Ball_Mass), it's the ratio, A_friction / A_normal.
    Measured empirically last year.-->
        <BALL_FRICTION>0.025</BALL_FRICTION>
<!--Vision reported confidence must be at least this amount or observation is
    ignored.-->
        <BALL_CONFIDENCE_THRESHOLD>0.1</BALL_CONFIDENCE_THRESHOLD>
<!--Time without observations before the ball can be considered occluded.-->
        <BALL_OCCLUDE_TIME>0.1</BALL_OCCLUDE_TIME>
<!--If improbability filtering is on, then this is the minimum likelihood 
    of a new observation or it is ignored.-->
        <BALL_LIKELIHOOD_THRESHOLD>0.0</BALL_LIKELIHOOD_THRESHOLD>
<!--# RobotTracker-->
<!--Output Kalman Errors-->
        <ROBOT_PRINT_KALMAN_ERROR>0</ROBOT_PRINT_KALMAN_ERROR><!--false-->        
<!--Use a closed form prediction based on current velocity for position.-->
        <ROBOT_FAST_PREDICT>1</ROBOT_FAST_PREDICT><!--true-->
<!--Maybe an improved method for propagating.-->
        <ROBOT_USE_AVERAGES_IN_PROPAGATION>0</ROBOT_USE_AVERAGES_IN_PROPAGATION><!--false-->
<!--Vision reported confidence must be at least this amount or observation is
    ignored.-->
        <ROBOT_CONFIDENCE_THRESHOLD>0.5</ROBOT_CONFIDENCE_THRESHOLD>
<!--Observation noises.-->
        <ROBOT_POSITION_VARIANCE>4.0</ROBOT_POSITION_VARIANCE><!--Stddev = 2mm-->
        <ROBOT_THETA_VARIANCE>0.00006</ROBOT_THETA_VARIANCE><!--Stddev = 0.5 degree-->
<!--Propagation noises.-->
        <ROBOT_NONE_VELOCITY_VARIANCE>250000.0</ROBOT_NONE_VELOCITY_VARIANCE><!--Stddev = 0.5m/s-->
        <ROBOT_NONE_ANGVEL_VARIANCE>0.0</ROBOT_NONE_ANGVEL_VARIANCE><!--StdDev = 0rad/s-->
        <ROBOT_DIFF_VELOCITY_VARIANCE>10000.0</ROBOT_DIFF_VELOCITY_VARIANCE><!--StdDev = 0.1m/s-->
        <ROBOT_DIFF_VELOCITY_VARIANCE_PERP>25.0</ROBOT_DIFF_VELOCITY_VARIANCE_PERP><!--StdDev = 5mm/s-->
        <ROBOT_DIFF_ANGVEL_VARIANCE>0.25</ROBOT_DIFF_ANGVEL_VARIANCE><!--StdDev = 0.5rad/s-->
        <ROBOT_OMNI_VELOCITY_VARIANCE>10000.0</ROBOT_OMNI_VELOCITY_VARIANCE><!--StdDev = 0.1m/s-->
        <ROBOT_OMNI_ANGVEL_VARIANCE>1.0</ROBOT_OMNI_ANGVEL_VARIANCE><!--StdDev = 1.0rad/s-->
        <ROBOT_STUCK_VARIANCE>0.25</ROBOT_STUCK_VARIANCE><!--Stddev = 0.5-->
<!--Sets the covariance of a robot's current velocity with its previous
    velocity.  This should either be 0 or 1.-->
        <ROBOT_VELOCITY_NEXT_STEP_COVARIANCE>1.0</ROBOT_VELOCITY_NEXT_STEP_COVARIANCE>
<!--Decays stuck over time so that a non moving robot will eventually be
    considered not stuck.-->
        <ROBOT_STUCK_DECAY>0.9</ROBOT_STUCK_DECAY>
<!--Threshold after which the robot is considered stuck and zero's velocity.-->
        <ROBOT_STUCK_THRESHOLD>0.6</ROBOT_STUCK_THRESHOLD><!--Set to 1.1 to turn off.-->
    </tracker>
</folder>
<!--contains config info for xdrive, ...-->    
