        <XDRIVE_KICKTIME>30000</XDRIVE_KICKTIME><!--us-->
<!--frame rate in hertz-->
        <XDRIVE_FRAMERATE>30</XDRIVE_FRAMERATE><!--Hz-->
</xclients>
</folder>
</xbel>         
